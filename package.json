    "espower-babel": "^1.3.0",
    "jsdoc-to-markdown": "^0.5.9",
    "markdown-to-ast": "^2.2.0",
    "mocha": "^2.1.0",
    "power-assert": "^0.10.0",
    "source-map-support": "^0.2.8",
    "textlint": "^2.0.2",
    "txt-ast-traverse": "^1.2.0",
